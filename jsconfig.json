{
	"compileOnSave": true,
	"compilerOptions": {
		"baseUrl": ".",
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"isolatedModules": false,
		"lib": [
			"esnext",
			"dom"
		],
		"paths": {
			"@/*": [
				"src/*"
			],
		},
	},
	"include": [
		"src/**/*.vue",
		"vite.config.js"
	],
	"exclude": [
		"node_modules",
	]
}