{
	"name": "chinadep-admin-template",
	"version": "0.0.1",
	"scripts": {
		"docs": "npx docsify serve docs -p 3001 --open",
		"dev": "vite --host",
		"build": "vue-tsc --noEmit && vite build",
		"serve": "vite preview",
		"prepare": "husky install",
		"commit": "git-cz",
		"czlog": "conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"dependencies": {
		"echarts": "^5.2.1",
		"element-plus": "^1.1.0-beta.15",
		"mitt": "^3.0.0",
		"normalize.css": "^8.0.1",
		"pinia": "^2.0.0-alpha.16",
		"unplugin-vue-components": "^0.15.2",
		"vue": "^3.0.11",
		"vue-echarts": "^6.0.0",
		"vue-router": "^4.0.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@types/node": "^16.9.6",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"@typescript-eslint/parser": "^4.31.2",
		"@vitejs/plugin-vue": "^1.9.0",
		"@vitejs/plugin-vue-jsx": "^1.1.8",
		"@vue/compiler-sfc": "^3.2.14",
		"commitizen": "^4.2.4",
		"conventional-changelog-cli": "^2.1.1",
		"cz-conventional-changelog": "^3.3.0",
		"docsify-cli": "^4.4.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-define-config": "^1.0.9",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^7.18.0",
		"husky": "^7.0.2",
		"lint-staged": "^11.1.2",
		"prettier": "^2.4.1",
		"rollup": "^2.57.0",
		"sass": "^1.42.1",
		"sass-loader": "^12.1.0",
		"typescript": "^4.4.3",
		"unplugin-element-plus": "^0.1.0",
		"vite": "^2.5.10",
		"vite-plugin-style-import": "^1.2.1",
		"vite-plugin-svg-icons": "^1.0.4",
		"vue-eslint-parser": "^7.11.0",
		"vue-tsc": "^0.3.0"
	},
	"lint-staged": {
		"src/**/*.{js,vue}": [
			"prettier --ignore-path .eslintignore --write",
			"eslint --cache --fix",
			"git add"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"license": "MIT"
}
